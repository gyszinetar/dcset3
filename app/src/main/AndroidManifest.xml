<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hu.prooktatas.dcset3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="hu.prooktatas.dcset3.ApplicationClass"
        android:allowBackup="true"
        android:icon="@drawable/chat"
        android:label="@string/app_name"
        android:roundIcon="@drawable/chat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="hu.prooktatas.dcset3.ui.login.LoginActivity"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="hu.prooktatas.dcset3.MainActivity"
            android:label="@string/title_activity"/>

    </application>

</manifest>